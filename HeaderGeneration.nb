Cell[BoxData["\<\"#define FROM_EXPRESSIONS_g_dd(dEsTiNaTiOn) \\\\\\n    \
dEsTiNaTiOn(1, 0, 0) = (DQ(0, 0)*(-1. + z)*(L * L)*(1. + z + z * z - 0.5*(mu \
* mu)*(z * z * z)))/(z * z); \\\\\\n    dEsTiNaTiOn(1, 1, 1) = (DQ(0, 1)*(L * \
L))/(z * z); \\\\\\n    dEsTiNaTiOn(1, 1, 3) = DQ(0, 4)*Q(1)*(L * L) + DQ(0, \
1)*Q(4)*(L * L); \\\\\\n    dEsTiNaTiOn(1, 2, 2) = (DQ(0, 2)*(L * L))/(z * \
z); \\\\\\n    dEsTiNaTiOn(1, 3, 1) = DQ(0, 4)*Q(1)*(L * L) + DQ(0, \
1)*Q(4)*(L * L); \\\\\\n    dEsTiNaTiOn(1, 3, 3) = ((L * L)*(2.*DQ(0, \
4)*Q(1)*Q(4)*(z * z * z * z) + DQ(0, 1)*(Q(4) * Q(4))*(z * z * z * z) + \
(2.*DQ(0, 3))/(2. - 1.*(2. + mu * mu)*(z * z * z) + (mu * mu)*(z * z * z * \
z))))/(z * z); \\\\\\n    dEsTiNaTiOn(3, 0, 0) = (Q(0)*(-1. + z)*(L * L)*(1. \
+ 2.*z - 1.5*(mu * mu)*(z * z)))/(z * z) + (Q(0)*(L * L)*(1. + z + z * z - \
0.5*(mu * mu)*(z * z * z)))/(z * z) + (DQ(1, 0)*(-1. + z)*(L * L)*(1. + z + z \
* z - 0.5*(mu * mu)*(z * z * z)))/(z * z) - (2.*Q(0)*(-1. + z)*(L * L)*(1. + \
z + z * z - 0.5*(mu * mu)*(z * z * z)))/(z * z * z); \\\\\\n    \
dEsTiNaTiOn(3, 1, 1) = (DQ(1, 1)*(L * L))/(z * z) - (2.*Q(1)*(L * L))/(z * z \
* z); \\\\\\n    dEsTiNaTiOn(3, 1, 3) = DQ(1, 4)*Q(1)*(L * L) + DQ(1, \
1)*Q(4)*(L * L); \\\\\\n    dEsTiNaTiOn(3, 2, 2) = (DQ(1, 2)*(L * L))/(z * z) \
- (2.*Q(2)*(L * L))/(z * z * z); \\\\\\n    dEsTiNaTiOn(3, 3, 1) = DQ(1, \
4)*Q(1)*(L * L) + DQ(1, 1)*Q(4)*(L * L); \\\\\\n    dEsTiNaTiOn(3, 3, 3) = \
((L * L)*(4.*Q(1)*(Q(4) * Q(4))*(z * z * z) - (2.*Q(3)*(-3.*(2. + mu * mu)*(z \
* z) + 4.*(mu * mu)*(z * z * z)))/((2. - 1.*(2. + mu * mu)*(z * z * z) + (mu \
* mu)*(z * z * z * z)) * (2. - 1.*(2. + mu * mu)*(z * z * z) + (mu * mu)*(z * \
z * z * z))) + 2.*DQ(1, 4)*Q(1)*Q(4)*(z * z * z * z) + DQ(1, 1)*(Q(4) * \
Q(4))*(z * z * z * z) + (2.*DQ(1, 3))/(2. - 1.*(2. + mu * mu)*(z * z * z) + \
(mu * mu)*(z * z * z * z))))/(z * z) - (2.*(L * L)*(Q(1)*(Q(4) * Q(4))*(z * z \
* z * z) + (2.*Q(3))/(2. - 1.*(2. + mu * mu)*(z * z * z) + (mu * mu)*(z * z * \
z * z))))/(z * z * z)\"\>"], "Output",
